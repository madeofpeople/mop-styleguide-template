{
  "name": "mop-styleguide-template",
  "version": "0.2.1",
  "description": "A twig builder made for Ggenerating styleguides using KSS",
  "main": "builder.js",
  "scripts": {
    "test": "kss --config=test/kss-config.json",
    "build": "cross-env NODE_ENV=development DEBUG=true node_modules/webpack/bin/webpack.js --progress && kss --config=test/kss-config.json",
    "build:production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress && kss --config=test/kss-config.json",
    "serve": "yarn build && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.23.0",
    "eslint-loader": "^4.0.2",
    "favicons-webpack-plugin": "^5.0.2",
    "highlight.js": "^10.7.1",
    "html-webpack-plugin": "^5.3.1",
    "kss": "^3.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "on-build-webpack": "^0.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^5.2.0",
    "remove-files-webpack-plugin": "^1.4.4",
    "robotstxt-webpack-plugin": "^7.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "sitemap-webpack-plugin": "^1.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge-and-include-globally": "^2.3.4",
    "webpackbar": "^5.0.0-3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/madeofpeople/mop-styleguide-template"
  }
}
